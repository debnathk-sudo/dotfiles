alias cbd="cd .."
alias sl="ls"
alias la="ls -a"
alias rmv="rm -rf"
alias add="git add -A"
alias commit="git commit -m 'alias commit'"
alias commit -m="git commit -m"
alias push="git push" 
alias pull="git pull"
alias charlie="add && commit && push"
alias running="ps -ax"
alias lls="ls -l"
alias base64="base64 --decode"
alias v="neofetch" 
alias kali="ssh carter@kalihome.local"
alias size="bash size.sh"
alias nmapscan="nmap --top-ports 1000 -T4 -sC"


export PATH=$HOME/repos/github.com/nolimitcarter/dotfiles/scripts:$PATH
